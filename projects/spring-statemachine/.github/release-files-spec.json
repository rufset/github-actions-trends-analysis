{
  "files": [
    {
      "aql": {
        "items.find": {
          "$and": [
            {
              "@build.name": "${buildname}",
              "@build.number": "${buildnumber}",
              "path": { "$match": "org/springframework/statemachine/spring-statemachine-*" }
            }
          ]
        }
      },
      "target": "nexus/"
    }
  ]
}
